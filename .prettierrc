{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tailwindFunctions": ["tw", "cn", "cva"],
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "quoteProps": "consistent",
    "arrowParens": "always",
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "proseWrap": "preserve"
}
